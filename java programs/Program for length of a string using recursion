public class len {

	public static void main(String[] args) {
      String str="chandan kumar";
      str=str+":";
      int i=-1;
      System.out.println(srl(str.toCharArray(),++i));
	}


public static int srl(char[] str,int i)
{
	int c=0;
	if(str[i]==':')
	{
		return i;
	}
	else
	{
		return srl(str,i=i+1);
	}
}
}
